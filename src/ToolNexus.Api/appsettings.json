{
  "Serilog": {
    "UseJsonConsole": true,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ]
  },
  "ToolResultCache": {
    "MaxEntries": 500,
    "AbsoluteExpirationSeconds": 300,
    "KeyPrefix": "toolnexus:tools"
  },
  "ConnectionStrings": {
    "Redis": ""
  },
  "Redis": {
    "Ssl": false,
    "InstanceName": "toolnexus"
  },
  "RateLimiting": {
    "IpPerMinute": 120,
    "UserPerMinute": 240
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  }
}
