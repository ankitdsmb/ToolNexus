{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LoggingOptions": {
    "EnableFileLogging": true,
    "EnableRuntimeLogCapture": true,
    "RuntimeLogEndpoint": "/api/admin/runtime/incidents/logs",
    "MinimumLevel": "Information",
    "RetentionDays": 14,
    "RuntimeDebugEnabled": false
  }
}
